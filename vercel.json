{
    "builds":[
        {
            "src": "index.js",
            "use":"@vercel/node"
        }
    ],

    "routes":[
        {
            "src": "/(.*)",
            "dest":"/index.js"
        }
    ],

    "rewrites": [
        {
            "destination": "/index.js",
            "source": "/(.*)"
        }
    ],
    "version": 2
}